
class Pair<K, V> {
    private K key;
    private V value;

    public Pair(K key, V value) {
        this.key = key;
        this.value = value;
    }

    public K getKey() {
        return key;
    }

    public V getValue() {
        return value;
    }

    public void setKey(K key) {
        this.key = key;
    }

    public void setValue(V value) {
        this.value = value;
    }
}

public class Main {
    public static void main(String[] args) {
        // Creating a Pair with String as key and Integer as value
        Pair<String, Integer> pair1 = new Pair<>("Age", 25);
        System.out.println("Key: " + pair1.getKey() + ", Value: " + pair1.getValue());

        // Creating a Pair with Character as key and Boolean as value
        Pair<Character, Boolean> pair2 = new Pair<>('A', true);
        System.out.println("Key: " + pair2.getKey() + ", Value: " + pair2.getValue());
    }
}
